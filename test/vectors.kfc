;; -*- scheme -*-

;; A test case making sure vectors are somewhat working.

(module
  (define (main)
      (let v (vector 1 2 3 4))
      (let len (length (var v)))
      (print (var len))
      (assert (= 4 (var len)))
      (print (vector-ref (var v) 0))
      (print (vector-ref (var v) 1))
      (print (vector-ref (var v) 2))
      (print (vector-ref (var v) 3))
      (assert (= 1 (vector-ref (var v) 0)))
      (assert (= 2 (vector-ref (var v) 1)))
      (assert (= 3 (vector-ref (var v) 2)))
      (assert (= 4 (vector-ref (var v) 3)))
      (return 0)))

(module
  (define (main)
    (let ((img (kernel ((i (iota 1024)))
                 (kernel ((j (iota 1024)))
                   (mod (* i j) 256)))))
      (print (length img))
      (print "\n")
      (print (length (vector-ref img 0)))
      (write-pgm "test.bin/test-image.pgm" img)
      (return 0))))
